                                                                                            <tr><td onclick="AddRemarks('No back tenderness;')">No back tenderness;</td></tr>
                                                                                            <tr><td onclick="AddRemarks('Both lower limbs have normal motor & sensory function;')">Normal Motor & sensory function;</td></tr>
                                                                                            <tr><td onclick="AddRemarks('No other abnorality detected from xray except scoliosis;')">No other abnorality detected from xray except scoliosis;</td></tr>
                                                                                        </table>
                                                                                    </div>
                                                                                </td>
                                                                            </tr>
                                                                        </table>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>

