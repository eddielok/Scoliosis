                                                &nbsp; &nbsp; <input type="button" value="Today" onClick="today()">
                                            </td>
                                            <th width="22%" bgcolor="RoyalBlue">Visit Type:</th>
                                            <td>
                                                <select id="Virtual" name="Virtual" onchange="ChangeVisitType()">

                                                    @if (Model.Item2.Virtual.ToString() == "Normal")
                                                    {
                                                        <option value="Normal" selected="selected">Normal </option>
                                                    }
                                                    else
                                                    {
                                                        <option value="Normal"> Normal</option>
                                                    }


                                                    @if (Model.Item2.Virtual.ToString() == "Pre-Clinic Measurement")
                                                    {
                                                        <option value="Pre-Clinic Measurement" selected="selected">Pre-Clinic Measurement</option>
                                                    }
                                                    else
                                                    {
                                                        <option value="Pre-Clinic Measurement">Pre-Clinic Measurement</option>
                                                    }

                                                    @if (Model.Item2.Virtual.ToString() == "Old X-Ray")
                                                    {
                                                        <option value="Old X-Ray" selected="selected">Old X-Ray</option>
                                                    }
                                                    else
                                                    {
                                                        <option value="Old X-Ray">Old X-Ray</option>
                                                    }
                                                </select>
                                            </td>
                                        </tr>
